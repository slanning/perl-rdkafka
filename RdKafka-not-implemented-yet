rd_kafka_event_topic_partition(rd_kafka_event_t *rkev)

rd_kafka_event_t *
rd_kafka_queue_poll(rd_kafka_queue_t *rkqu, int timeout_ms)

#endif  /* RD_KAFKA_VERSION >= 0x000902ff */
